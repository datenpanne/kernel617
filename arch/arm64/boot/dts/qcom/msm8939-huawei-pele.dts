/dts-v1/;

#include "msm8939-pm8916.dtsi"
#include "msm8916-modem-qdsp6.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/leds/common.h>
#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>
#include <dt-bindings/sound/apq8016-lpass.h>
#include <dt-bindings/pinctrl/qcom,pmic-mpp.h>

/ {
	model = "Huawei MediaPad T2 Pro 7";
	compatible = "huawei,pele", "qcom,msm8939";
	chassis-type = "tablet";

	aliases {
		mmc0 = &sdhc_1; /* eMMC */
		mmc1 = &sdhc_2; /* SD card */
		serial0 = &blsp_uart2;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	reserved-memory {
		cont_splash_mem: framebuffer@83000000 {
			compatible = "removed-dma-pool";
			reg = <0x0 0x83000000 0x0 0x1A00000>;
			no-map;
		};
/*
		reserved@85000000 {
			reg = <0x0 0x85000000 0x0 0x1000000>;
			no-map;
		};*/

		qseecom_mem: qseecom@84a00000 {
			reg = <0x0 0x84a00000 0x0 0x1600000>;
			no-map;
		};

		tz-hwlog@8c700000 {
			reg = <0x0 0x8c700000 0x0 0x400000>;
			no-map;
		};

		ramoops@8cb00000 {
			compatible = "ramoops";
			reg = <0x0 0x8cb00000 0x0 0x200000>;

			record-size = <0x10000>;
			console-size = <0x100000>;
			ftrace-size = <0>;
		};
	};

	battery: battery {
		compatible = "simple-battery";
		device-chemistry = "lithium-ion-polymer";
		voltage-min-design-microvolt = <3300000>;
		voltage-max-design-microvolt = <4350000>;
		energy-full-design-microwatt-hours = <16074000>;
		charge-full-design-microamp-hours = <4230000>;

		//precharge-current-microamp = <1300000>;
		//charge-term-current-microamp = <150000>;

		ocv-capacity-celsius = <25>;
		ocv-capacity-table-0 = <4330000 100>, <4265000 95>,
			<4208000 90>, <4153000 85>, <4100000 80>, <4049000 75>,
			<4001000 70>, <3962000 65>, <3919000 60>, <3872000 55>,
			<3839000 50>, <3817000 45>, <3798000 40>, <3783000 35>,
			<3767000 30>, <3747000 25>, <3729000 20>, <3709000 16>,
			<3688000 13>, <3681000 11>, <3680000 10>, <3679000 9>,
			<3677000 8>, <3674000 7>, <3666000 6>, <3641000 5>,
			<3597000 4>, <3537000 3>, <3457000 2>, <3336000 1>,
			<3000000 0>;
	};

	gpio-hall-sensor {
		compatible = "gpio-keys";

		pinctrl-0 = <&gpio_hall_sensor_default>;
		pinctrl-names = "default";

		label = "GPIO Hall Effect Sensor";

		event-hall-sensor {
			label = "Hall Effect Sensor";
			gpios = <&tlmm 69 GPIO_ACTIVE_LOW>;
			linux,input-type = <EV_SW>;
			linux,code = <SW_LID>;
			linux,can-disable;
		};
	};

	gpio-keys {
		compatible = "gpio-keys";

		pinctrl-0 = <&gpio_keys_default>;
		pinctrl-names = "default";

		label = "GPIO Buttons";

		button-volume-up {
			label = "Volume Up";
			gpios = <&tlmm 107 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_VOLUMEUP>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		pinctrl-0 = <&gpio_leds_default>;
		pinctrl-names = "default";

		led-0 {
			gpios = <&tlmm 8 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_RED>;
			default-state = "off";
			function = LED_FUNCTION_INDICATOR;
			retain-state-suspended;
		};

		led-1 {
			gpios = <&tlmm 9 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_GREEN>;
			default-state = "off";
			function = LED_FUNCTION_INDICATOR;
			retain-state-suspended;
		};

		led-2 {
			gpios = <&tlmm 1 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_BLUE>;
			default-state = "off";
			function = LED_FUNCTION_INDICATOR;
			retain-state-suspended;
		};
	};

	reg_lcd_vddio: regulator-lcd-vddio {
		compatible = "regulator-fixed";
		regulator-name = "vddio-incell";
		vin-supply = <&pm8916_l16>;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
		regulator-boot-on;
	};

	reg_lcd_vsp: regulator-lcd-vsp {
		compatible = "regulator-fixed";
		regulator-name = "vsp";
		regulator-min-microvolt = <5400000>;
		regulator-max-microvolt = <5400000>;
		regulator-always-on;
		regulator-boot-on; // allows fb console to show up briefly at boot without always-on
		enable-active-high;

		/*gpios = <&tlmm 97 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		startup-delay-us = <200000>; //delay 200
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_vsp_default>;*/
	};

	reg_lcd_vsn: regulator-lcd-vsn {
		compatible = "regulator-fixed";
		regulator-name = "vsn";
		regulator-min-microvolt = <5400000>;
		regulator-max-microvolt = <5400000>;
		regulator-always-on;
		regulator-boot-on; // allows fb console to show up briefly at boot without always-on
		enable-active-high;

	/*	gpios = <&tlmm 32 GPIO_ACTIVE_HIGH>;
		//startup-delay-us = <200000>; //delay 200
		enable-active-high;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_vsn_default>;*/
	};

	reg_tp_vdd: regulator-tp-vdd@112 {
		compatible = "regulator-fixed";
		regulator-name = "tp-vdd";
		gpios = <&tlmm 112 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-always-on;
		regulator-boot-on;
	};

	reg_tp_vddio: regulator-tp-vddio@10 {
		compatible = "regulator-fixed";
		regulator-name = "tp-vddio";
		vin-supply = <&pm8916_l16>;
		gpios = <&tlmm 10 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-always-on;
		regulator-boot-on;
	};

/*	reg_tp_vdd: regulator-tp-vdd {
		compatible = "regulator-fixed";
		regulator-name = "tp-vdd";
		vin-supply = <&pm8916_l17>;
		regulator-min-microvolt = <2850000>;
		regulator-max-microvolt = <2850000>;
		regulator-always-on;
		regulator-boot-on;

		gpios = <&tlmm 112 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		//startup-delay-us = <200000>; //delay 200
		pinctrl-names = "default";
		pinctrl-0 = <&tp_vdd_default>;
	};

	reg_tp_vddio: regulator-tp-vddio {
		compatible = "regulator-fixed";
		regulator-name = "tp-vddio";
		vin-supply = <&pm8916_l16>;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
		regulator-boot-on; 

		gpios = <&tlmm 10 GPIO_ACTIVE_HIGH>;
		//enable-active-high;
		//startup-delay-us = <200000>; //delay 200
		pinctrl-names = "default";
		pinctrl-0 = <&tp_vddio_default>;
	};*/

	usb_id: usb-id {
		compatible = "linux,extcon-usb-gpio";
		id-gpios = <&tlmm 118 GPIO_ACTIVE_HIGH>;
		pinctrl-0 = <&usb_id_default>;
		pinctrl-names = "default";
	};
};

&blsp_i2c2 {
	status = "okay";

	accelerometer@18 {
		compatible = "bosch,bma255";
		reg = <0x18>;

		interrupt-parent = <&tlmm>;
		interrupts = <115 IRQ_TYPE_EDGE_RISING>;

		vdd-supply = <&pm8916_l17>;
		vddio-supply = <&pm8916_l6>;

		mount-matrix = "0", "1", "0",
			      		       "-1", "0", "0",
			       		       "0", "0", "1";

		pinctrl-0 = <&accel_int_default>;
		pinctrl-names = "default";
	};

	magnetometer@c {
		compatible = "asahi-kasei,ak09911";
		reg = <0x0c>;
		vdd-supply = <&pm8916_l17>;
		vid-supply = <&pm8916_l6>;
		reset-gpios = <&tlmm 36 GPIO_ACTIVE_LOW>;
		pinctrl-0 = <&mag_reset_default>;
		pinctrl-names = "default";
	};

	/*proximity@1e { // reference
		compatible = "ap3426";
		reg = <0x1e>;
	};*/
};

&blsp_i2c4 {
	status = "okay";
	charger: bq24190@6b {
		compatible = "ti,bq24296";
		reg = <0x6b>;
		interrupt-parent = <&tlmm>;
		interrupts = <62 IRQ_TYPE_EDGE_FALLING>;
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&bq24192_charger_default>;
		pinctrl-1 = <&bq24192_charger_sleep>;
		monitored-battery = <&battery>;
		//ti,system-minimum-microvolt = <3350000>;
		omit-battery-class;
		usb_otg_vbus: usb-otg-vbus {
		};
	};
};

&blsp_i2c5 {
	status = "okay";
	clock-frequency = <400000>;

	touchscreen@24 {
		compatible = "cypress,tt21000";
		reg = <0x24>;

		interrupt-parent = <&tlmm>;
		interrupts = <13 IRQ_TYPE_EDGE_FALLING>;

		reset-gpios = <&tlmm 12 GPIO_ACTIVE_HIGH>;

		vdd-supply = <&reg_tp_vdd>;
		vddio-supply = <&reg_tp_vddio>;

		pinctrl-0 = <&touchscreen_default>;
		pinctrl-1 = <&touchscreen_sleep>;
		pinctrl-names = "default","sleep";
	};
};

&blsp_uart2 {
	status = "okay";
};

&gpu {
	status = "okay";
};
/*
&lpass {
	status = "okay";
};

&lpass_codec {
	status = "okay";
};*/

&mdss {
	status = "okay";
};

&mdss_dsi0 {
	pinctrl-0 = <&mdss_default &mdss_te_default>;
	pinctrl-1 = <&mdss_sleep &mdss_te_sleep>;
	pinctrl-names = "default", "sleep";
	status = "okay";
	//vddio-supply = <&pm8916_l6>;

	panel@0 {
		reg = <0>;
		compatible = "huawei,pele-panel";

		reset-gpios = <&tlmm 25 GPIO_ACTIVE_LOW>;
		disp-te-gpios = <&tlmm 24 GPIO_ACTIVE_HIGH>;

		vddio-supply = <&pm8916_l6>;
		vddio-incell-supply = <&pm8916_l16>;
		vsp-supply = <&reg_lcd_vsp>;
		vsn-supply = <&reg_lcd_vsn>;
		vsp-gpios = <&tlmm 97 GPIO_ACTIVE_HIGH>;
		vsn-gpios = <&tlmm 32 GPIO_ACTIVE_HIGH>;
		vcc-gpios = <&tlmm 2 GPIO_ACTIVE_HIGH>;
		backlight-gpios = <&tlmm 3 GPIO_ACTIVE_HIGH>;
		vled-gpios = <&tlmm 109 GPIO_ACTIVE_HIGH>;

		port {
			panel_in: endpoint {
				remote-endpoint = <&mdss_dsi0_out>;
			};
		};
	};
};

&mdss_dsi0_out {
	data-lanes = <0 1 2 3>;
	remote-endpoint = <&panel_in>;
};

&mdss_dsi0_phy {
	qcom,dsi-phy-regulator-ldo-mode;
	status = "okay";
};

&mpss_mem {
	reg = <0x0 0x86800000 0x0 0x5500000>;
};

&pm8916_bms {
	monitored-battery = <&battery>;
	power-supplies = <&charger>;

	status = "okay";
};

&pm8916_charger {
	monitored-battery = <&battery>;
	status = "okay";
};

&pm8916_codec {
	qcom,micbias-lvl = <2800>;
	qcom,mbhc-vthreshold-low = <75 150 237 450 500>;
	qcom,mbhc-vthreshold-high = <75 150 237 450 500>;
	qcom,micbias2-ext-cap;
	qcom,hphl-jack-type-normally-open;
	status = "okay";
};
